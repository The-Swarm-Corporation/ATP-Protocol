# Agentic Trade Protocol (ATP) Gateway Configuration

# =============================================================================
# REQUIRED
# =============================================================================

# Swarms API Key for agent execution
SWARMS_API_KEY=your_swarms_api_key_here

# Solana wallet public key for receiving agent payments
AGENT_TREASURY_PUBKEY=your_solana_wallet_pubkey_here

# =============================================================================
# OPTIONAL (with defaults)
# =============================================================================

# Solana RPC endpoint
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Redis connection URL for job storage
REDIS_URL=redis://localhost:6379/0

# Job time-to-live in seconds (default: 600 = 10 minutes)
JOB_TTL_SECONDS=600

# Logging level
LOG_LEVEL=INFO
